<div class="Sec2-rankItem Sec2-rankItem--second">
    <div class="Sec2-rankAvatar Sec2-rankAvatar--second">
        <img class="Sec2-rankImage Sec2-rankImage--second" src="{{prizeList[1].jyImg}}" alt="{{prizeList[1].uname}}" width="120" height="120">
        <div class="Sec2-rankPlace Sec2-rankPlace--second"><span>{{prizeList[1].rank}}</span></div>
        <div class="Sec2-rankStatus Sec2-rankStatus--second {{prizeList[1].hourRankChange}}"></div>
    </div>
    <p class="Sec2-rankName">
        {{if prizeList[1].jyFirstKingDarkHorse}}
        <span class="Sec2-rankExtra" title="错过2015年星光盛典首王资格，本次活动豪刷250万荣誉值进场的黑马选手！"></span>
        {{/if}}
        <span title="{{prizeList[1].uname}}">
            {{prizeList[1].uname}}
        </span>
    </p>
    <p class="Sec2-rankHonor">
        荣誉值：{{prizeList[1].value}}
    </p>
</div>
